   "source": [
    "import numpy as np \n",
    "#1.正常的談話的聲壓為20000微巴斯卡，請問多少分貝?\n",
    "#請寫下程式\n",
    "V1 = 20000\n",
    "V0 = 20\n",
    "gdb = 20*(np.log10(V1/V0))\n",
    "print(gdb)\n",
    "#2.30分貝的聲壓會是50分貝的幾倍?\n",
    "#公式移項過後可以得到 V1 = ?\n",
    "#請寫下程式\n",
    "def REgdb(v1):\n",
    "    muPa = 20*10**(v1/20)\n",
    "    return muPa\n",
    "v1 = 50\n",
    "v2 = 30\n",
    "ans = REgdb(v1) / REgdb(v2)\n",
    "print(ans)"
   ]
  }
